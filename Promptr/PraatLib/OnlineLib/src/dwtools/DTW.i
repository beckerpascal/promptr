  xmin = me -> xmin; xmax = me -> xmax;
 }
 if (ymin >= ymax)
 {
  ymin = me -> ymin; ymax = me -> ymax;
 }

 if (! Matrix_getWindowSamplesY (me, ymin, ymax, &iymin, &iymax) ||
  ! Matrix_getWindowSamplesX (me, xmin, xmax, &ixmin, &ixmax)) return;

 Graphics_setInner (g);
 Graphics_setWindow (g, xmin, xmax, ymin, ymax);

 while (ipmin < me -> pathLength && me -> path[ipmin].x < ixmin) ipmin++;

 ipmax = ipmin;

 while (ipmax < me -> pathLength && me -> path[ipmax].x <= ixmax) ipmax++;

 x1 = Matrix_columnToX (me, me -> path[ipmin].x);
 y1 = Matrix_rowToY (me, me -> path[ipmin].y);

 for (i = ipmin + 1; i <= ipmax; i++)
 {
  x2 = Matrix_columnToX (me, me -> path[i].x);
  y2 = Matrix_rowToY (me, me -> path[i].y);
  Graphics_line (g, x1, y1, x2, y2);
  x1 = x2;
  y1 = y2;
 }

 Graphics_unsetInner (g);
 if (garnish)
 {
  Graphics_drawInnerBox(g);
  Graphics_marksBottom (g, 2, 1, 1, 0);
  Graphics_marksLeft (g, 2, 1, 1, 0);
 }
}

