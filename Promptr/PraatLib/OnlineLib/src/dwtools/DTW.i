void DTW_drawDistancesAlongPath (DTW me, Any g, double xmin, double xmax,
 double dmin, double dmax, int garnish)
{
 long i, ixmax, ixmin;
 float *d = ((void *)0);

 if (! (d = NUMfvector (1, me -> nx))) return;

 if (xmin >= xmax)
 {
  xmin = me -> xmin; xmax = me -> xmax;
 }
 if( ! Matrix_getWindowSamplesX (me, xmin, xmax, &ixmin, &ixmax)) return;

 i = 1;
 while (i < me -> pathLength && me -> path[i].x < ixmin) i++;
 ixmin = i;

 while (i <= me -> pathLength && me -> path[i].x < ixmax) i++;
 ixmax = i;

 if ((d = NUMfvector (ixmin, ixmax)) == ((void *)0)) return;

 for (i = ixmin; i <= ixmax; i++)
 {
  d[i] = me -> z[me -> path[i].y][i];
 }

 if (dmin >= dmax)
 {
  NUMfvector_extrema (d, ixmin, ixmax, &dmin, &dmax);
 }
 else
 {
  for (i = ixmin; i <= ixmax; i++)
  {
   if (d[i] > dmax)
   {
    d[i] = dmax;
   }
   else if (d[i] < dmin)
   {
    d[i] = dmin;
   }
  }
 }

 Graphics_setInner (g);
 Graphics_setWindow (g, xmin, xmax, dmin, dmax);
 Graphics_function (g, d, ixmin, ixmax, xmin, xmax);
 Graphics_unsetInner (g);

 if (garnish)
 {
  Graphics_drawInnerBox(g);
  Graphics_textLeft (g, 1, "distance");
  Graphics_marksBottom (g, 2, 1, 1, 0);
  Graphics_marksLeft (g, 2, 1, 1, 0);
 }

 NUMfvector_free (d, ixmin);
}




DTW Matrices_to_DTW (Any void_me, Any void_thee, int matchStart, int matchEnd, int slope, int metric)
{
 Matrix me = (Matrix) void_me; Matrix thee = (Matrix) void_thee;
 DTW him = ((void *)0);
 long i, j, k;

 if (thee -> ny != me -> ny) return Melder_errorp ("Matrices_to_DTW: Columns must have the same dimensions.");

 him = DTW_create (me -> xmin, me -> xmax, me -> nx, me -> dx, me -> x1, thee -> xmin, thee -> xmax, thee -> nx, thee -> dx, thee -> x1);
 if (him == ((void *)0)) return ((void *)0);

 for (i = 1; i <= me -> nx; i++)
 {
  for (j = 1; j <= thee -> nx; j++)
  {




   double dmax = 0, d = 0, dtmp;
   for (k = 1; k <= me -> ny; k++)
   {
    dtmp = fabs (me -> z[k][i] - thee -> z[k][j]);
    if (dtmp > dmax) dmax = dtmp;
   }
   if (dmax > 0)
   {
    for (k = 1; k <= me -> ny; k++)
    {
     dtmp = fabs (me -> z[k][i] - thee -> z[k][j]) / dmax;
     d += pow (dtmp, metric);
    }
   }
   d = dmax * pow (d, 1.0 / metric);
   him -> z[i][j] = d / me -> ny;
  }
  if ((i % 10) == 1 && ! Melder_progress (0.999 * i / me -> nx,
    "Calculate distances: column %ld from %ld", i, me -> nx)) goto end;
 }
 DTW_findPath (him, matchStart, matchEnd, slope);
end:
 Melder_progress (1.0, ((void *)0));
 if (Melder_hasError ()) _Thing_forget ((Thing *) & (him));
 return him;
}

DTW Spectrograms_to_DTW (Spectrogram me, Spectrogram thee, int matchStart,
 int matchEnd, int slope, int metric)
{
 Matrix m1 = ((void *)0), m2 = ((void *)0);
 DTW him = ((void *)0);
 long i, j;

 if (me -> xmin != thee -> xmin || me -> ymax != thee -> ymax || me -> ny != thee -> ny)
 {
   return Melder_errorp("Spectrograms_to_DTW: #frequencies and/or "
    "frequency ranges do not match.");
 }
 if (((m1 = Spectrogram_to_Matrix (me)) == ((void *)0)) ||
  ((m2 = Spectrogram_to_Matrix (thee)) == ((void *)0)) ||
  ((him = DTW_create (me -> xmin, me -> xmax, me -> nx, me -> dx, me -> x1,
   thee -> xmin, thee -> xmax, thee -> nx, thee -> dx, thee -> x1)) == ((void *)0))) goto end;





 for (i = 1; i <= me -> ny; i++)
 {
  for (j = 1; j <= me -> nx; j++)
  {
   m1 -> z[i][j] = 10 * log10 (m1 -> z[i][j]);
  }
 }
 for (i = 1; i <= thee -> ny; i++)
 {
  for (j = 1; j <= thee -> nx; j++)
  {
   m2 -> z[i][j] = 10 * log10 (m2 -> z[i][j]);
  }
 }

 him = Matrices_to_DTW (m1, m2, matchStart, matchEnd, slope, metric);

end:
 _Thing_forget ((Thing *) & (m1));
 _Thing_forget ((Thing *) & (m2));
 if (Melder_hasError ()) _Thing_forget ((Thing *) & (him));
 return him;
}




static int Pitch_findFirstAndLastVoicedFrame (Pitch me, long *first, long *last)
{
 *first = 1;
 while (*first <= me -> nx && ! Pitch_isVoiced_i (me, *first)) (*first)++;
 *last = me -> nx;
 while (*last >= *first && ! Pitch_isVoiced_i (me, *last)) (*last)--;
 return *first <= me -> nx && *last >= 1;
}

