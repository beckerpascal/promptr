DTW Pitches_to_DTW_sgc (Pitch me, Pitch thee, double vuv_costs, double time_weight, int matchStart, int matchEnd, int slope)
{
 DTW him = ((void *)0);
 long i, j, myfirst, mylast, thyfirst, thylast;
 double *pitchx = ((void *)0);
 int unit = 5;

  if (vuv_costs < 0) return Melder_errorp ("Voiced-unvoiced costs may not be negative.");
  if (time_weight < 0) return Melder_errorp ("Time costs weight may not be negative.");

 if (! Pitch_findFirstAndLastVoicedFrame (me, &myfirst, &mylast) ||
  ! Pitch_findFirstAndLastVoicedFrame (thee, &thyfirst, &thylast)) return Melder_errorp
   ("No voiced frames.");






  him = DTW_create (me -> xmin, me -> xmax, me -> nx, me -> dx, me -> x1, thee -> xmin, thee -> xmax, thee -> nx, thee -> dx, thee -> x1);
  if (him == ((void *)0)) return ((void *)0);
  pitchx = NUMdvector (1, thee -> nx);
  if (pitchx == ((void *)0)) goto end;
  for (j = 1; j <= thee -> nx; j++)
  {
   pitchx[j] = Sampled_getValueAtSample (thee, j, 1, unit);
  }

  for (i = 1; i <= me -> nx; i++)
  {
   double pitchy = Sampled_getValueAtSample (me, i, 1, unit);
   double t1 = me -> x1 + (i - 1) * me -> dx;
   for (j = 1; j <= thee -> nx; j++)
   {
    double t2 = thee -> x1 + (j - 1) * thee -> dx;
    double dist_f = 0;
    double dist_t = fabs (t1 -t2);
    if (pitchy == (__builtin_huge_val()))
    {
     if (pitchx[j] != (__builtin_huge_val()))
     {
      dist_f = vuv_costs;
     }
    }
    else if (pitchx[j] == (__builtin_huge_val()))
    {
     dist_f = vuv_costs;
    }
    else
    {
     dist_f = fabs(pitchy - pitchx[j]);
    }
    him -> z[i][j] = sqrt (dist_f * dist_f + time_weight * dist_t * dist_t);
   }
  }
  DTW_findPath (him, matchStart, matchEnd, slope);
end:
 NUMdvector_free (pitchx, 1);
  if (Melder_hasError ()) _Thing_forget ((Thing *) & (him));
  return him;
}

